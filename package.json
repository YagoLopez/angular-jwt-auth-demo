{
  "name": "@yagolopez/angular-jwt-auth-demo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "ng serve -o",
    "copy-lib-assets": "cpx ./src/assets/**/* ./dist/angular-jwt-auth/assets",
    "build-app": "ng build --base-href ./",
    "build-lib": "ng build angular-jwt-auth",
    "build-lib-with-assets": "npm run build-lib && npm run copy-lib-assets",
    "build-lib-with-assets2": "run-s build-lib copy-lib-assets",
    "build-lib-watch": "ng build angular-jwt-auth --watch",
    "test-app": "ng test angular-jwt-auth-demo",
    "test-lib": "ng test angular-jwt-auth",
    "e2e": "run-p serve cypress",
    "cypress": "./node_modules/.bin/cypress open",
    "cypress-record-video": "cypress run --record --key 818feff0-23f3-4e94-8af8-60cf250b63ef",
    "coverage": "ng test angular-jwt-auth --watch=false --code-coverage",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular-devkit/build-ng-packagr": "^0.8.4",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~4.2.1",
    "cpx": "^1.5.0",
    "cypress": "^3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.4.0",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "wallaby-webpack": "^3.9.13"
  }
}
